{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",

		
		"target": "es5",
		"allowJs": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		
		"skipLibCheck": true,
		"sourceMap": true
	},
	"include": ["**/*.ts"],
	"exclude": [
		"**/*.spec.ts",
		"node_modules"
	]
}